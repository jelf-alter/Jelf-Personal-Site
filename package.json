{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.ts",
    "test:property": "vitest run --reporter=verbose src/**/*.property.test.ts",
    "test:components": "vitest run --reporter=verbose src/components/**/*.test.ts",
    "test:stores": "vitest run --reporter=verbose src/stores/**/*.test.ts",
    "test:utils": "vitest run --reporter=verbose src/utils/**/*.test.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "fast-check": "^3.15.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.25"
  }
}
